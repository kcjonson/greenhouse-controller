{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run watch",
    "start": "npm run build && node dist/server/index.js",
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack",
    "build-server": "mkdir -p build && babel src/server --out-dir dist/server -s --ignore src/**/*.spec.js",
    "watch": "concurrently --raw npm:watch-* --kill-others-on-fail",
    "watch-client": "npm run build-client -- --watch",
    "watch-server-src": "npm run build-server -- --watch",
    "watch-server-build": "nodemon --watch dist/server dist/server/index.js",
    "clean": "rm -rf build",
    "lint": "eslint . --ext .js,.jsx,.tsx",
    "test": "npm run start"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redis": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "directory-named-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^4.5.0",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.2.2",
    "mini-svg-data-uri": "^1.3.3",
    "nodemon": "^2.0.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.11.1",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-cli": "^4.3.0"
  }
}
